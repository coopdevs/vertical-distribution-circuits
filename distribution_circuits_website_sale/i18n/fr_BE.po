msgid "<?xml version=\"1.0\"?>\n"
"<div style=\"margin:auto;width:100%;\">\n"
"          <img src=\"/web/image/res.company/${user.company_id.id}/logo\" style=\"height: auto; width: 80px; margin-top:30px;\" alt=\"${user.company_id.name}\"/>\n"
